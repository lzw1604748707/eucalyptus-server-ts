{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug-app",
      "type": "node",
      "request": "launch",
      "args": [
        "${workspaceRoot}/src/app.ts" // 入口文件
      ],
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/ts-node-dev",
      "restart": true,
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
